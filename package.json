  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "eslint": "^4.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "flow-bin": "^0.50.0",
    "npm-run-all": "^4.0.2",
    "npm-watch": "^0.2.0"
  }
}
