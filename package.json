{
  "name": "any-cfg",
  "version": "0.7.0",
  "author": "phaux <phaux4@gmail.com>",
  "description": "Config loader",
  "repository": "phaux/node-any-cfg",
  "license": "ISC",
  "keywords": [
    "config",
    "loader"
  ],
  "main": "lib",
  "module": "module",
  "typings": "module",
  "scripts": {
    "prepare": "npm run prepare:module && npm run prepare:lib",
    "prepare:module": "tsc -p . -m esnext -t es2018 --outDir module",
    "prepare:lib": "tsc -p . -m commonjs -t es2017 --outDir lib",
    "prepublishOnly": "npm test",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "tslint -p .",
    "test:unit": "eslint test && tsc -p tsconfig.test.json && node test"
  },
  "devDependencies": {
    "@types/node": "^9.6.6",
    "eslint": "^4.19.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
