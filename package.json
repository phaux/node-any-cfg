{
  "name": "any-cfg",
  "version": "0.1.0",
  "author": "phaux <phaux4@gmail.com>",
  "description": "Config loader",
  "repository": "phaux/node-any-cfg",
  "license": "ISC",
  "keywords": [
    "config",
    "loader"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "run-p -l test:*",
    "test:lint": "eslint src test --color && echo All good ðŸ‘Œ",
    "test:flow": "flow --quiet --color=always",
    "test:unit": "babel-node test/index.js",
    "build": "run-s -l build:*",
    "build:babel": "babel src -d lib",
    "prepublish": "npm run -s build",
    "watch": "npm-watch"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "quiet": true
    }
  },
  "dependencies": {
